  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-2": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "node-fetch": "^1.3.3",
    "node-static": "^0.7.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-thunk": "^1.0.0",
    "uglify": "^0.1.5",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "redux-thunk": "^1.0.0"
  }
